{
  "name": "tradingview-access-management-node",
  "version": "2.0.0",
  "description": "API RESTful ultrarrápida para gestión masiva de acceso a scripts de TradingView",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:cluster": "node src/cluster.js",
    "dev": "nodemon src/server.js",
    "dev:cluster": "nodemon src/cluster.js",
    "dev:dashboard": "cd dashboard && npm run dev",
    "build:dashboard": "cd dashboard && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:dashboard\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:bulk": "node scripts/real-users-test.js",
    "test:performance": "node scripts/bulk-test.js",
    "test:cluster": "MAX_WORKERS=2 npm run test",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "pm2:start": "pm2 start src/cluster.js --name tradingview-api",
    "pm2:stop": "pm2 stop tradingview-api",
    "pm2:restart": "pm2 restart tradingview-api",
    "pm2:delete": "pm2 delete tradingview-api",
    "smart-test": "node scripts/smart-bulk-test.js",
    "controlled-test": "node scripts/controlled-test.js",
    "status": "node scripts/status-check.js",
    "test:10": "node scripts/test-10-users.js",
    "calibrate": "node scripts/calibration-test.js",
    "test:adaptive": "node scripts/test-remove-grant.js",
    "apply:adaptive": "node scripts/apply-adaptive-config.js",
    "diagnose": "node scripts/diagnose-issue.js"
  },
  "keywords": [
    "tradingview",
    "api",
    "bulk-access",
    "nodejs",
    "express",
    "high-performance"
  ],
  "author": "diazpolanco13",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "nodemailer": "^7.0.6",
    "pino": "^8.16.0",
    "pino-pretty": "^10.3.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diazpolanco13/Tradingview-Access-Management-base.git"
  }
}